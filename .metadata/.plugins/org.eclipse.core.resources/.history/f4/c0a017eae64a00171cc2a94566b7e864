package com.kaitzen;

import java.util.*;

public class Cuenta {
	private float amount;
	private String name;
	private Integer id;
	private Random RDID = new Random();
	private static Integer MIN_SEED = 1;
	private static Integer MAX_SEED = 1000;
	
	public Cuenta() {
		Integer ramdonid = getRdId();
		
	}
	
	public Integer getId() {
		return id;
	}
	 
	public void setId(Integer id) {
		this.id = id;
	}
	
	public float getAmount() {
		return amount;
	}
	
	public void setAmount(float amount) {
		this.amount = amount;
	}
	
	public String getName() {
		return name;
	}
	
	public void setNanme(String name) {
		this.name = name;
	}
	

	public void reintegro(){
		
	}
	
	public void ingreso(){
		
	}
	
	public void transferencia(){
		
	}
	
	public Integer getRdId(){
		return RDID.nextInt(MAX_SEED - MIN_SEED) - MIN_SEED;
	}
}
